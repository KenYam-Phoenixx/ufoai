				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-DWINVER=0x501" />
					<Add option="-DNODEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
