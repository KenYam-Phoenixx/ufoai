// useable parameter for amours defines which team can use it: 0 - human, 1 - alien
/*	steelblade is here for the combat knife
	monomolecularblade is here for the kerrblade (and similar)
	it ensures that the superior tech blades are much better against armour
	and keeps the kerrblade scary
*/

// if the damage type starts with _ it is marked at translateable and thus shown
// in the menus, if there is no such character the damagetype will not be shown.
damagetypes standard
{
	"_normal"
	"normal_light"
	"normal_spray"
	"normal_medium"
	"normal_heavy"
	"normal_power"
	"_steelblade"
	"_monomolecularblade"
	"_blast"
	"_fire"
	"fire_light"
	"fire_medium"
	"fire_heavy"
	"fire_flamer"
	"_shock"
	"_laser"
	"laser_light"
	"laser_medium"
	"laser_heavy"
	"laser_power"
	"_plasma"
	"plasma_light"
	"plasma_medium"
	"plasma_heavy"
	"plasma_power"
	"_particlebeam"
	"particlebeam_light"
	"particlebeam_medium"
	"particlebeam_heavy"
	"particlebeam_power"
	"_stun"
}

