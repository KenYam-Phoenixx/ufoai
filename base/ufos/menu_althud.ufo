	// CHAT BUTTONS
	// ==================
	pic bar_chat
	{
		if	"maxclients >= 2"
		image	hud/bar_chat
		pos		"1024 580"
		align	"ur"
		out	{
				*bar_chat image hud/bar_chat
				*bar_chat size "64 64"
				cmd "mn_showchatbuttons 0"
			}
		in	{
				*bar_chat image hud/bar_chat_choice
				*bar_chat size "256 64"
				cmd "mn_showchatbuttons 1"
			}
	}
	pic bar_chat_all
	{
		if	"mn_showchatbuttons == 1"
		tooltip	"_Chat"
		image	hud/button_chat
		pos		"976 592"
		align	"ur"
		texl	"0 0"
		texh	"32 32"
		click {
				cmd	"messagesay"
			}
	}
	pic bar_chat_team
	{
		if	"mn_showchatbuttons == 1"
		tooltip	"_Teamchat"
		image	hud/button_chat
		pos		"938 592"
		align	"ur"
		texl	"32 0"
		texh	"64 32"
		click {
				cmd	"messagesayteam"
			}
	}
	pic bar_chat_show
	{
		if	"mn_showchatbuttons == 1"
		tooltip	"_Show chat history"
		image	hud/button_chat
		pos		"900 592"
		align	"ur"
		texl	"64 0"
		texh	"96 32"
		mousefx false
		out	{
				*chatscreen size "690 48"
				*chatscreen height "3"
				*event timeout 10000
			}
		in	{
				*chatscreen size "690 400"
				*chatscreen height "25"
				*event timeout 0
				cmd "mn_unhidenode chatscreen"
			}
	}

	pic button_chat
	{
		if	"maxclients >= 2"
		tooltip	"_Chat messages"
		image	menu/event_log
		texl	"0 0"
		texh	"32 36"
		pos	"985 590"
		mousefx false
		in
	}

